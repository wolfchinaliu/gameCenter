<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@include file="/context/mytags.jsp" %>
<!DOCTYPE html>
<html>
<head>
    <meta charset=utf-8>
    <!-- 优先使用最新版本 IE 和 Chrome -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no"/>
    <meta content="yes" name="apple-mobile-web-app-capable"/>
    <meta content="black" name="apple-mobile-web-app-status-bar-style"/>
    <meta name="format-detection" content="telphone=no, email=no"/>
    <title>商家简介</title>
    <link rel="stylesheet" type="text/css" href="plug-in/liuliangbao/css/0204/lib/normalize.css">
    <link rel="stylesheet" type="text/css" href="plug-in/liuliangbao/css/0204/index.css">
    <script src="plug-in/liuliangbao/js/0201/common.js"></script>
</head>
<body>
<article class="container">
    <!--页头-->
    <header class="header">
        <h1 class="content-title1">${title}</h1>
    </header>

    <section class="main content-main">

        <c:forEach var="item" items="${content}">
            <p class="content-p">
                ${item}
            </p>
        </c:forEach>

    </section>

</article>


</body>
</html>
