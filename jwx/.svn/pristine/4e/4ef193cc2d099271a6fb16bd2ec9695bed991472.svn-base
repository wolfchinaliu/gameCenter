@charset "UTF-8";
/*重定义标签*/
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td
{ margin:0; padding:0; }
/*定义body,必须给颜色,不然在开启健康护色后会很是绿色*/
body{color:#333; position: relative; top: 0; left: 0;}
/*必须定义input之类的,这些东西居然不继承, 他妹的*/
body, button, input, select, textarea{font: 12px/1.8 'Microsoft YaHei',tahoma,arial,serif;}
ol, ul { list-style: none;}
blockquote, q { quotes: none; /*margin: 0 40px;*/}
blockquote:before, blockquote:after,
q:before, q:after { content: none;}
textarea{ overflow:auto; resize:none;/*让其不可以改变大小*/}
/* 元素获得焦点时的样式! */
:focus { outline: 0;}
/* 特殊文本的样式! */
ins { text-decoration: none;}
del { text-decoration: line-through;}
/* 细线表格样式 */
table { border-collapse: collapse; border-spacing: 0;}
/*定义连接*/
a{text-decoration:none;color:#333;}
a:hover{color:#27bd5c;}
a.current{color:#27bd5c;}
/* 解决按钮要比正常宽的bug*/
button,input{ overflow: visible;}
input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {
    -webkit-box-shadow : 0 0 0px 1000px white inset ;
}
/*清除浮动*/
.clx{zoom: 1;}
.clx:after{ clear:both; content:"."; display:block; height:0; visibility:hidden; overflow:hidden;}
/*定义浮动*/
.fl{float:left;}
.fr{float:right;}
/*定义小三角*/
.arrow{display:inline-block; border:5px dashed transparent; height: 0; width:0; overflow:hidden;}
/*让横向隐藏 html{ min-width: 1170px;}*/
