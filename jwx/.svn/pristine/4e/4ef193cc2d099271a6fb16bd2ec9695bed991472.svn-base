h1,h2,h3,h4,h5,h6{font-weight: normal;font-size:100%;}
i{font-style: normal;}
b{font-weight: normal;}

/*解决图片上/下有边界*/
img{vertical-align: top;border:none;}

/*解决li在ie7里有上边界*/
li{vertical-align:top;}

/*文字方向定位*/
.tl{ text-align: left;}
.tr{ text-align: right;}
.tc{ text-align: center;}

/*线性块级元素*/
.none{ display: none;}
.db{ display: block;}
.dib{ display: inline-block;}
.di{ display: inline;}

/*定义一个总宽*/

.w{width: 1170px; margin: 0 auto;}
/*布局*/
.container { width: 100%; background-color: #fff;}
.main { width: 1170px; margin: 0 auto; padding-top: 20px; padding-bottom: 50px;}
.main-l { width: 242px; float: left;}
.main-r { width: 730px; float: right;}

/*loading*/
.icons-loading {width: 16px;height: 16px; background-image: url(../../images/loading-16-16.gif);}

/*
* 时间轴
*/
.ui-timeline-vertical { padding-left: 10px; padding-top: 20px; background-color: #fff;}
.ui-timeline-vertical li { position: relative; top: 0; left: 0; min-height: 20px; padding-bottom: 20px;}
.ui-timeline-vertical li .icons { position: absolute; top: -7px; left: -7px;}
.ui-timeline-vertical li.ui-timeline-past { border-left: 1px solid #1cafff;}
.ui-timeline-vertical li.ui-timeline-current { border-left: 1px solid #1cafff;}
.ui-timeline-vertical li.ui-timeline-future { border-left: 1px solid #e2e3e3;}
.ui-timeline-vertical li.ui-timeline-end {  border-left: 1px solid #fff; }
.ui-timeline-vertical-li-l { width: 100px; padding-left: 20px; font-size: 16px; color: #777; float: left; margin-top: -13px;}
.ui-timeline-vertical-li-r { width: 500px; float: left; font-size: 14px; color: #b7b6b6; margin-top: -13px;}
.ui-timeline-vertical-li-r span{ margin-right: 40px; }
.ui-timeline-vertical li.ui-timeline-current p { color: #1cafff;}

.ui-timeline-horizontal { height: 50px; margin-top: 29px; margin-left: 15px;}
.ui-timeline-horizontal li { display: inline-block; height: 40px; position: relative; top: 0; left: 0; float: left; text-align: center;}
.ui-timeline-horizontal li .icons { position: absolute; left: 50%; top: -7px; margin-left: -6px; z-index: 100;}
.ui-timeline-horizontal li p { height: 20px; padding: 20px 10px 0 10px; line-height: 20px; text-align: center; font-size: 14px; color: #777;}
.ui-timeline-horizontal .timeline-xian { display: inline-block; height: 0; width: 100%; border-top: 1px solid #ccc; font-size: 0; overflow: hidden; position: absolute; top: 0; right: 0;}
.ui-timeline-horizontal .timeline-xian-l { width: 50%; left: 0;}
.ui-timeline-horizontal .timeline-xian-r { width: 50%; right: 0;}
.ui-timeline-horizontal li.timeline-past p {color: #999;}
.ui-timeline-horizontal li.timeline-current p {color: #999;}
.ui-timeline-horizontal li.timeline-future p {color: #bfbfbf;}
.ui-timeline-horizontal .timeline-past .timeline-xian {  border-top: 1px solid #1cafff;}
.ui-timeline-horizontal .timeline-current .timeline-xian-l {  border-top: 1px solid #1cafff; }
.ui-timeline-horizontal .timeline-future .timeline-xian {  border-top: 1px solid #ccc; }

/*
* 输入框样式
*/
.ui-text { height: 16px; line-height: 16px; padding: 11px 5px; border: 1px solid #c9c9c9; border-radius: 3px; vertical-align: middle;}
.ui-text-l { width: 50px; text-align: center; border-bottom-right-radius: 0; border-top-right-radius: 0; border-right: none;}
.ui-text-r { width: 200px; border-top-left-radius: 0; border-bottom-left-radius: 0;}
.ui-area { width: 400px; height: 100px; padding: 5px; line-height: 2em; border: 1px solid #c9c9c9; border-radius: 3px; vertical-align: top;}

/*
* 面包屑
*/
.crumbs { height: 20px; line-height: 20px; font-size: 14px; color: #585858;}
.crumbs span,.crumbs a { color: #585858;}
.crumbs a:hover { color: #1cafff;}
.crumbs i { padding: 0 5px; color: #888;}

/*提示文字*/
.msg-error {  color: #fd790f}

/*表单验证的错误提示*/
.valid-tips { height: 20px; line-height: 20px; color: #666; display: inline-block; color: #585858;}
.valid-error { color: #fd790f; }
.valid-success { color: #20a14d; }
.valid-tips .icons-1,.valid-tips .icons-2 { margin-right: 5px; vertical-align: -3px; *vertical-align: middle; }
label.error { height: 20px; line-height: 20px; color: #fd790f; margin-left: 10px; padding-left: 20px;
    background: url(../../images/error.png) no-repeat left center; }
/*div.error { display: none; }*/
input.checkbox { border: none }
/*input:focus { border: 1px dotted black; }*/
input.error,select.error,textarea.error { border: 1px solid #fd790f; }

/*导航标题*/
.ui-navigation { height: 22px; padding: 15px 0; border-bottom: solid 1px #e1e1e1; line-height: 22px; font-size: 18px;
 color: #000;}
/*标签*/
.ui-label { display: inline-block; position: relative; top: 0; left: 0; border-radius: 3px; background-color: #fff;}
.ui-label-1 { border: solid 1px #006600; height: 22px; line-height: 22px; padding: 0 10px;}
.ui-label .icons { display: none; position: absolute; top: 3px; right: 0;}
.ui-label:hover .icons { display: block;}

