@charset "UTF-8";


/*弹出层*/

.ui-dialog-close,.wed-dialog-tips-icon{background-image:url('../../images/lib/ui-dialog-1.0.png');background-repeat:no-repeat;}
/*弹出层*/
.ui-dialog-outer { text-align:left; outline:none;font-size:14px;position:relative;}
.ui-dialog-border{ border:0 none; margin:0; border-collapse:collapse;}
/*.ui-dialog-header, .ui-dialog-footer { padding:0;  border-top: 2px solid #9bc357;}*/
.ui-dialog-iframe .ui-dialog-content{height:100%;background:#fff url('../../images/loading-32-32.gif') center center no-repeat;}
.ui-dialog-iframe iframe{width: 100%;height: 100%;}
/*标题栏*/
.ui-dialog-header{ position:relative;/*  height:36px; */ z-index:100; background-color: #ffffff;}
.ui-dialog-title { overflow:hidden;cursor:default;height:42px; line-height:42px; text-align: center;color:#333;/*border-top:2px solid  #9bc357;*/border-bottom: 1px solid #e6e6e6;font-size: 18px;}
.ui-dialog-close{display:block; outline:none;position: absolute;overflow:hidden;right: 10px;top: 10px;height: 15px;width: 15px;text-indent:-9999em;cursor:pointer;background-position: 0 0;}
.ui-dialog-close:hover{text-decoration:none;background-position:0 -15px;}
/*loading*/
.ui-dialog-loading {min-width:80px; padding-right: 30px;height: 50px;overflow: hidden;background:#fff url('../../images/loading-32-32.gif') no-repeat 30px center;line-height: 50px;padding-left: 80px;font-size: 16px;}
/*按钮组*/
.ui-dialog-buttons{padding:20px; text-align:center; white-space:nowrap;font-size:0;}
.ui-dialog-button{ margin-left:15px;cursor: pointer; display: inline-block; text-align: center;height:30px;line-height:30px;padding:0 24px;width:auto;border: 1px solid #ccc; background-color:#fff;color:#666;outline:none;font-size:14px;border-radius: 2px}
.ui-dialog-buttons a:first-child{margin-left:0;}
.ui-dialog-button:hover{color:#666;background-color:#f3f3f3;text-decoration:none;}
/*无效按钮*/
.ui-dialog-button-disabled,.ui-dialog-button-disabled:hover,.ui-dialog-button-disabled:active{cursor:default;color:#666;background-color:#ddd;border-color:#999;filter:alpha(opacity=50); opacity:.5;text-decoration:none;}
/*高亮*/
.ui-dialog-button-on{background-color:#ff7900;color:#fff;border-color:#ff7900;}
.ui-dialog-button-on:hover{background-color:#ff6c11;color:#fff;border-color:#ff6c11;}
/*单个按钮*/
.ui-dialog-button-only{padding: 0 80px; }
/*遮罩*/
.ui-dialog-mask { background-color:#000; filter:alpha(opacity=30); opacity:0.3; }
.ui-dialog-inner{background-color:#fff;border:1px solid #bab0b8; -webkit-box-shadow: 2px 2px 6px 0px rgba(0,0,0,.4);
    -moz-box-shadow: 2px 2px 6px 0px rgba(0,0,0,.4);
    box-shadow: 2px 2px 6px 0px rgba(0,0,0,.4); }
/*没有标题*/
.ui-dialog-noTitle .ui-dialog-title { display:none; }
/*视觉*/
@media screen and (min-width:0) {
    .ui-dialog-outer { -webkit-transform: scale(0); transform: scale(0); -webkit-transition: -webkit-transform .2s ease-in-out; transition: transform .2s ease-in-out; }
    .ui-dialog-visible .ui-dialog-outer{ -webkit-transform: scale(1); transform: scale(1); }
}
/*扩展开始*/
/*alert*/
.wed-dialog-alert .ui-dialog-content{text-align:center;padding-top:30px;padding-bottom:20px; background-color: #fafafa;}
.wed-dialog-alert .ui-dialog-noTitle .ui-dialog-content{padding-top:40px;}
.wed-dialog-alert .ui-dialog-content h3{font-size:16px;color:#111;}
.wed-dialog-alert .ui-dialog-content p{color:#999;font-size:12px;}
.wed-dialog-alert .ui-dialog-inner{border:none;}
/*简单提示*/
.wed-dialog-tips .ui-dialog-border{border-color:#ccc;}
.wed-dialog-tips .ui-dialog-content{padding:16px 30px 16px 38px;color:#333;}
.wed-dialog-tips .ui-dialog-title-outer{display:none;}
.wed-dialog-tips-icon{position:absolute;left:16px;top:20px;overflow:hidden;display:block;width:16px;height:16px;}
.wed-dialog-tips-success-icon{background-position:0 -62px;}
.wed-dialog-tips-warning-icon{background-position:0 -46px;}
.wed-dialog-tips-error-icon{background-position:0 -30px;}
/*loading*/
.wed-dialog-loading .ui-dialog-header{display:none;}
/*tips*/
.ui-dialog-tips-inner{position:relative;z-index:90;}

/*v6的吸附*/
.ui-dialog-top-left .ui-dialog-outer,
.ui-dialog-top .ui-dialog-outer,
.ui-dialog-top-right .ui-dialog-outer {
    top: -16px;
}
.ui-dialog-bottom-left .ui-dialog-outer,
.ui-dialog-bottom .ui-dialog-outer,
.ui-dialog-bottom-right .ui-dialog-outer {
    top: 6px;
}
.ui-dialog-left-top .ui-dialog-outer,
.ui-dialog-left .ui-dialog-outer,
.ui-dialog-left-bottom .ui-dialog-outer {
    left: -16px;
}
.ui-dialog-right-top .ui-dialog-outer,
.ui-dialog-right .ui-dialog-outer,
.ui-dialog-right-bottom .ui-dialog-outer {
    left: 16px;
}
.ui-dialog-arrow-a,
.ui-dialog-arrow-b {
    position: absolute;
    display: none;
    width: 0;
    height: 0;
    overflow:hidden;
    border:8px dashed transparent;
}

.ui-dialog-follow .ui-dialog-arrow-b,.ui-dialog-follow .ui-dialog-arrow-a{display:block;}
.ui-dialog-top-left .ui-dialog-arrow-a,
.ui-dialog-top .ui-dialog-arrow-a,
.ui-dialog-top-right .ui-dialog-arrow-a {
    bottom: -16px;
    border-top:8px solid #bab0b8;
}
.ui-dialog-top-left .ui-dialog-arrow-b,
.ui-dialog-top .ui-dialog-arrow-b,
.ui-dialog-top-right .ui-dialog-arrow-b {
    bottom: -15px;
    border-top:8px solid #fff;
}
.ui-dialog-top-left .ui-dialog-arrow-a,
.ui-dialog-top-left .ui-dialog-arrow-b  {
    left: 15px;
}
.ui-dialog-top .ui-dialog-arrow-a,
.ui-dialog-top .ui-dialog-arrow-b  {
    left: 50%;
    margin-left: -8px;
}
.ui-dialog-top-right .ui-dialog-arrow-a,
.ui-dialog-top-right .ui-dialog-arrow-b {
    right: 15px;
}
.ui-dialog-bottom-left .ui-dialog-arrow-a,
.ui-dialog-bottom .ui-dialog-arrow-a,
.ui-dialog-bottom-right .ui-dialog-arrow-a {
    top: -16px;
    border-bottom:8px solid #bab0b8;
}
.ui-dialog-bottom-left .ui-dialog-arrow-b,
.ui-dialog-bottom .ui-dialog-arrow-b,
.ui-dialog-bottom-right .ui-dialog-arrow-b {
    top: -15px;
    border-bottom:8px solid #fff;
}
.ui-dialog-bottom-left .ui-dialog-arrow-a,
.ui-dialog-bottom-left .ui-dialog-arrow-b {
    left: 15px;
}
.ui-dialog-bottom .ui-dialog-arrow-a,
.ui-dialog-bottom .ui-dialog-arrow-b {
    margin-left: -8px;
    left: 50%;
}
.ui-dialog-bottom-right .ui-dialog-arrow-a,
.ui-dialog-bottom-right .ui-dialog-arrow-b {
    right: 15px;
}
.ui-dialog-left-top .ui-dialog-arrow-a,
.ui-dialog-left .ui-dialog-arrow-a,
.ui-dialog-left-bottom .ui-dialog-arrow-a {
    right: -16px;
    border-left:8px solid #bab0b8;
}
.ui-dialog-left-top .ui-dialog-arrow-b,
.ui-dialog-left .ui-dialog-arrow-b,
.ui-dialog-left-bottom .ui-dialog-arrow-b {
    right: -15px;
    border-left:8px solid #fff;
}
.ui-dialog-left-top .ui-dialog-arrow-a,
.ui-dialog-left-top .ui-dialog-arrow-b {
    top: 15px;
}
.ui-dialog-left .ui-dialog-arrow-a,
.ui-dialog-left .ui-dialog-arrow-b {
    margin-top: -8px;
    top: 50%;
}
.ui-dialog-left-bottom .ui-dialog-arrow-a,
.ui-dialog-left-bottom .ui-dialog-arrow-b {
    bottom: 15px;
}
.ui-dialog-right-top .ui-dialog-arrow-a,
.ui-dialog-right .ui-dialog-arrow-a,
.ui-dialog-right-bottom .ui-dialog-arrow-a {
    left: -16px;
    border-right:8px solid #bab0b8;
}
.ui-dialog-right-top .ui-dialog-arrow-b,
.ui-dialog-right .ui-dialog-arrow-b,
.ui-dialog-right-bottom .ui-dialog-arrow-b {
    left: -15px;
    border-right:8px solid #fff;
}
.ui-dialog-right-top .ui-dialog-arrow-a,
.ui-dialog-right-top .ui-dialog-arrow-b {
    top: 15px;
}
.ui-dialog-right .ui-dialog-arrow-a,
.ui-dialog-right .ui-dialog-arrow-b {
    margin-top: -8px;
    top: 50%;
}
.ui-dialog-right-bottom .ui-dialog-arrow-a,
.ui-dialog-right-bottom .ui-dialog-arrow-b {
