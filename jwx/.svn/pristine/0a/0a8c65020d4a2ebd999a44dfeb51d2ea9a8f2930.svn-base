    <%--$.ajax({--%>
      <%--url: "weixinGuessRiddlesController.do?importExcel&accountid=${accountid}&username=${username }&hdid=${hdid}",--%>
      <%--type: "GET",--%>
      <%--dataType: 'json',--%>
      <%--success: function (data) {--%>
