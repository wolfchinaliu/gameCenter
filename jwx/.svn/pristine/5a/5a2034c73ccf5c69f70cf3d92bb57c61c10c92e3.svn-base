/*导航*/
.ui-title { height: 40px; border-bottom: solid 1px #d5d5d5;}
.ui-title span { display: inline-block; height: 36px; line-height: 36px; padding: 0 20px;}
.ui-title1 { font-size: 18px; color: #1ebc94;}
.ui-title1 span { border-bottom: solid 4px #1ebc94; position: relative; bottom: -1px;}
.ui-title a { display: inline-block; height: 36px; line-height: 36px; padding: 0 20px; color: #666;}
.ui-title a:hover { border-bottom: solid 4px #1ebc94; position: relative; color: #000; }
.ui-title a.on { border-bottom: solid 4px #1ebc94; position: relative; color: #000;}
/*标签*/
.ui-label { display: inline-block; border: 1px solid #D5D5D5; border-radius: 5px; position: relative; top: 0; left: 0; 
	vertical-align: middle; z-index: 1;}
.ui-label a { display: none; position: absolute; width: 20px; height: 48px; top: 0; right: 5px; text-align: center;}
.ui-label:hover a { display: block;}
.ui-label-1 { background-color: #1ebc94; font-size: 18px; color: #fff; height: 48px; line-height: 48px; padding: 0 25px;}
.ui-label-1 a { color: #fff;}

/*模拟下拉框*/
.ui-select { width: 398px; height: 48px; border: 1px solid #d5d5d5; background-color: #fff; position: relative; top: 0;
	 left: 0; z-index: 1000;}
.ui-select-con { width: 308px; height: 48px; line-height: 48px; padding: 0 20px; font-size: 14px; color: #666666; float: left;}
.ui-select-btn { display: block; width: 25px; height: 12px; padding: 18px 13px 18px 12px; float: right; background-color: #f5f5f5; cursor: pointer;}
.ui-select-btn .icons { vertical-align: top;}
.ui-select-xl {width: 398px; max-height: 230px; position: absolute; top: 48px; left: -1px; border: 1px solid #d5d5d5;
 	background-color: #fff; z-index: 100; overflow-x: hidden; overflow-y: scroll; display: none;}
.ui-select-list { }
.ui-select-list li { height: 34px; padding-left: 20px; line-height: 34px; cursor: pointer;}
.ui-select-list li:hover { background-color: #1ebc94; color: #fff;}
.ui-select-add { border-top: 1px solid #d5d5d5; height: 34px; padding: 7px 0; background-color: #f5f5f5;}
.ui-select-add .ui-text { margin-left: 30px; width: 198px; padding: 8px 5px; float: left;}
.ui-select-add .btns { padding: 0 25px; height: 34px; line-height: 34px; float: right; margin-right: 30px;}









