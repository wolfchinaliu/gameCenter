* {
	PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
A {
	OUTLINE-STYLE: none; COLOR: #555; TEXT-DECORATION: none
}
A:hover {
	COLOR: #3fa9f5
}
A {
	
}
UL {
	LIST-STYLE-TYPE: none
}
H4 {
	FONT-WEIGHT: normal
}
H3 {
	FONT-WEIGHT: normal
}
H2 {
	FONT-WEIGHT: normal
}
H1 {
	FONT-WEIGHT: normal
}
IMG {
	BORDER-BOTTOM-STYLE: none; BORDER-LEFT-STYLE: none; BORDER-TOP-STYLE: none; BORDER-RIGHT-STYLE: none
}
HTML {
	MIN-WIDTH: 1000px; FONT: 14px/24px "풕턼환쥌 Light", "풕턼환쥌",arial,sans-serif; BACKGROUND: #fff; HEIGHT: 100%; COLOR: #666
}
BODY {
	MIN-WIDTH: 1000px; FONT: 14px/24px "풕턼환쥌 Light", "풕턼환쥌",arial,sans-serif; BACKGROUND: #fff; HEIGHT: 100%; COLOR: #666
}
.l {
	FLOAT: left
}
.r {
	FLOAT: right
}
.g {
	MARGIN: 0px auto; WIDTH: 1000px
}
#header {
	BACKGROUND: url(../../images/green/header_bg.png) #fff repeat-x; HEIGHT: 100px
}
#header .g {
	HEIGHT: 100px
}
#logo A {
	WIDTH: 290px; DISPLAY: block; BACKGROUND: url(logo_weixin.png) no-repeat; HEIGHT: 100px
}
#area {
	WIDTH: 110px; HEIGHT: 100px
}
#areaBtn {
	TEXT-ALIGN: center; WIDTH: 110px; DISPLAY: block; BACKGROUND: url(../images/logo_weixin.png) no-repeat right 0px; COLOR: #b3b3b3; FONT-SIZE: 22px; CURSOR: pointer; PADDING-TOP: 42px
}
#areaBtn:hover {
	BACKGROUND-POSITION: right -100px; COLOR: #3fa9f5
}
#nav {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; HEIGHT: 40px; PADDING-TOP: 40px
}
#nav LI {
	FLOAT: left; HEIGHT: 40px; FONT-SIZE: 18px
}
#nav LI A {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 30px; PADDING-RIGHT: 30px; PADDING-TOP: 0px
}
#nav LI A.on {
	COLOR: #44af35
}
#loginBox {
	WIDTH: 142px; HEIGHT: 36px; PADDING-TOP: 35px
}
#loginBox A {
	TEXT-INDENT: -9999px; WIDTH: 70px; DISPLAY: inline-block; FLOAT: left; HEIGHT: 36px; MARGIN-RIGHT: 1px
}
#loginBox A.loginBtn:hover {
	BACKGROUND-POSITION: 0px -36px
}
#loginBox A.regBtn {
	BACKGROUND-POSITION: right 0px
}
#loginBox A.regBtn:hover {
	BACKGROUND-POSITION: right -36px
}
#crumbs {
	PADDING-BOTTOM: 3px; PADDING-LEFT: 0px; WIDTH: 990px; PADDING-RIGHT: 0px; FONT-SIZE: 12px; PADDING-TOP: 3px
}
#catfocus {
	POSITION: relative; DISPLAY: none; HEIGHT: 160px; OVERFLOW: hidden
}
.catfocus {
	POSITION: absolute; WIDTH: 2000em; HEIGHT: 160px
}
.catfocus LI {
	WIDTH: 1000px; FLOAT: left; HEIGHT: 160px
}
.filterBox {
	POSITION: relative; PADDING-BOTTOM: 2px; LINE-HEIGHT: 40px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; BACKGROUND: url(../../images/green/filter_bg.png) no-repeat; HEIGHT: 40px; PADDING-TOP: 2px
}
.filterBox H1 {
	POSITION: absolute; FONT-SIZE: 17px; FONT-WEIGHT: normal; TOP: 2px; LEFT: 15px
}
.filterBtn {
	MARGIN: 0px auto; WIDTH: 280px; HEIGHT: 40px
}
.filterBtn LI {
	TEXT-ALIGN: center; WIDTH: 70px; FLOAT: left; CURSOR: pointer
}
.filterBtn LI.active {
	BACKGROUND: url(../../images/green/filterBtn_bg.png) no-repeat 0px 0px; COLOR: #fff
}
.listType {
	POSITION: absolute; TEXT-ALIGN: right; WIDTH: 80px; HEIGHT: 40px; RIGHT: 15px; TOP: 2px
}
#footer {
	BACKGROUND: #44af35; HEIGHT: 200px; COLOR: #fff; BORDER-TOP: #3ea02d 1px solid
}
#footer .g {
	OVERFLOW: auto
}
.ftNav {
	TEXT-ALIGN: center; WIDTH: 490px; PADDING-TOP: 40px
}
.ftNav A {
	COLOR: #fff
}
.copyright {
	BACKGROUND: url(../../images/green/copyright.png) no-repeat center 50%; HEIGHT: 120px
}
.code {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 37px
}
.close {
	BACKGROUND-IMAGE: url(../../images/green/close.png); POSITION: absolute; WIDTH: 50px; HEIGHT: 50px; CURSOR: pointer; RIGHT: 10px; TOP: 10px
}
.close:hover {
	BACKGROUND-POSITION: 0px -50px
}
#overlay {
	BACKGROUND-IMAGE: url(../../images/green/transparent.png); PADDING-BOTTOM: 20px; PADDING-LEFT: 20px; WIDTH: 800px; PADDING-RIGHT: 20px; DISPLAY: none; HEIGHT: 560px; OVERFLOW: hidden; PADDING-TOP: 20px
}
DIV.overlayTool {
	HEIGHT: 60px
}
.oImgBox {
	HEIGHT: 60px; MARGIN-RIGHT: 10px
}
.oImgBox IMG {
	WIDTH: 60px; BACKGROUND: #ccc; HEIGHT: 60px; border-radius: 30px
}
.oTitle H3 {
	PADDING-BOTTOM: 0px; LINE-HEIGHT: 28px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; FONT-SIZE: 26px; PADDING-TOP: 5px
}
.oTitle P {
	COLOR: #aaa
}
.oLink {
	PADDING-RIGHT: 35px
}
.oLink A {
	LINE-HEIGHT: 30px; PADDING-LEFT: 30px; DISPLAY: inline-block; BACKGROUND: url(../../images/green/oLink.png) no-repeat; FLOAT: left; COLOR: #c3c4c0; MARGIN-RIGHT: 10px
}
.oLink A:hover {
	COLOR: #3fa9f5
}
.oLink A.allM:hover {
	BACKGROUND-POSITION: 0px -30px
}
.oLink A.newW {
	BACKGROUND-POSITION: 0px -60px
}
.oLink A.newW:hover {
	BACKGROUND-POSITION: 0px -90px
}
DIV.contentWrap {
	OVERFLOW-Y: auto; MARGIN-TOP: 15px; WIDTH: 800px; BACKGROUND: #f5f5f5; HEIGHT: 484px; border-radius: 5px
}
#service {
	POSITION: fixed; MARGIN-TOP: -67px; WIDTH: 36px; HEIGHT: 216px; RIGHT: 0px; TOP: 50%; _position: absolute
}
#service A {
	TEXT-INDENT: -9999px; WIDTH: 36px; DISPLAY: block; BACKGROUND: url(../../images/green/service.png) no-repeat; FLOAT: left; HEIGHT: 108px; CURSOR: pointer
}
#service .srvLog {
	
}
#service .srvLog:hover {
	BACKGROUND-POSITION: -36px 0px
}
#service .srvCns {
	BACKGROUND-POSITION: 0px -108px
}
#service .srvCns:hover {
	BACKGROUND-POSITION: -36px -108px
}
