/*my common*/
html { font-size: 100px; width: 100%; height: 100%; }
body {font-family:"Microsoft YaHei","Helvetica Neue",Helvetica,Arial,sans-serif; font-size: 0.16rem; width: 100%; background: url(../../images/follow/bg01.jpg); }
html, body { -webkit-text-size-adjust: none;}
ul, ol { list-style:none; margin: 0; padding: 0;} 
figure { margin: 0;}
/*清除浮动*/
.clx{zoom: 1;}
.clx:after{ clear:both; content:"."; display:block; height:0; visibility:hidden; overflow:hidden;}
/*定义浮动*/
.fl{float:left;}
.fr{float:right;}
.t-center { text-align: center;}
input:focus,select:focus { outline: none;}
dl, dd {margin: 0;}


.text { width: 3.5rem; height: 0.5rem; line-height: 0.5rem; border: none; border-bottom: solid 1px #666666; background-color: rgba(0,0,0,0);}
.btn { display: inline-block; border: none; text-align: center; font-size: 0.34rem; text-decoration: none;}
.btn-1 { height: 0.78rem; line-height: 0.78rem; color: #FFFFFF; background-color: #fa6958; width: 100%; }

.red { color: #fa6958;}
.c999 { color: #999999;}
.c666 { color: #666666;}

.ui-btn { height: 0.68rem; line-height: 0.68rem; text-align: center; font-size: 0.34rem; background-color: #fa6958; border: none; display: block; color: #fff; text-decoration: none;}

.container { width: 7.5rem; margin: 0 auto;}
/*header*/
.header {width: 7.5rem; height: 2.7rem; overflow: hidden; background: url(plug-in/liuliangbao/20160701/images/follow/header_bg.jpg) no-repeat left top; background-size: contain;}
.header-user {padding-top: 0.28rem; height: 1.4rem;}
.header-user figure { width: 1.36rem; height: 1.36rem; border: solid 0.02rem #fff; float: left; margin-left: 0.67rem; border-radius: 50%; overflow: hidden;}
.header-user figure img { width: 1.36rem; height: 1.36rem; border-radius: 50%;}
.header-user > dl { width: 4.9rem; float: left; padding-left: 0.45rem; padding-top: 0.1rem;}
.header-user > dl dt { font-size: 0.36rem; line-height: 1.8em;}
.header-user > dl dd { font-size: 0.28rem; line-height: 1.8em;}
.header-msg { font-size: 0.3rem; padding-top: 0.2rem; text-align: center;}
.header-msg > span { display: inline-block; padding: 0 0.4rem;}

/*footer*/
.footer { width: 7.5rem; height: 0.85rem;}
.footer img { width: 100%;}

/*banner*/
.banner { width: 7.16rem; margin: 0 auto; padding-top: 0.5rem;}
.banner img { width: 100%;}

/*弹框*/
.overlay { width: 100%; height: 100%; position: fixed; top: 0; left: 0; z-index: 500; background-color: #000000;}
.tips-pop { width: 80%; height: 0.6rem; line-height: 0.6rem; text-align: center; font-size: 0.3rem; color: #ffffff; background-color: #000000; position: fixed; top: 40%; left: 10%; z-index: 501;}

.dialog { width: 5.96rem; height: 6.65rem; position: absolute; top: 2rem; left: 0.77rem; z-index: 501; background: url(plug-in/liuliangbao/20160701/css/follow/bg1.png) no-repeat left top; background-size: contain; display: none;}
.dialog-close { display: block; width: 0.45rem; height: 0.45rem; position: absolute; top: 0; right: 0.1rem; text-indent: -10000px; overflow: hidden;}
.dialog-header { height: 2.52rem;}
.dialog-header-title1 { text-align: center;}
.dialog-header-title1 img { width: 3.75rem; height: 1.6rem;}
.dialog-header-title2 { height: 0.5rem; line-height: 0.5rem; text-align: center; font-size: 0.38rem; color: #FA6958;}
.dialog-main { padding-left: 0.35rem;}
.dialog-main h3 {padding-top: 0.2rem; height: 0.4rem;}
.dialog-main h3 span { display: inline-block; height: 0.4rem; line-height: 0.4rem; background-color: #292929; color: #ffffff; padding: 0 0.1rem;}
.dialog-list { padding-top: 0.2rem;}
.dialog-list li { width: 2.8rem; height: 0.5rem; line-height: 0.5rem; float: left; color: #FA6958; font-size: 0.32rem; overflow: hidden;}
.dialog-list li span {display: inline-block; width: 50%; float: left;}
.dialog-msg {font-size: 0.32rem; color: #FA6958; padding-top: 0.2rem;}


























