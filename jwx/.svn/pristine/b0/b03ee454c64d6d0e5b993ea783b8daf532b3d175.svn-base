    display: none;
}
.award-info .tabs li{
    -webkit-box-flex:1;
    box-flex:1;
    color: #ffffff;
    font-size: 1.4rem;
    width: 100%;
    position: relative;
    margin: 0 1rem;
    text-align: center;
}
.award-info .tabs li:before{
    content: '';
    display: block;
    -webkit-border-radius: .6rem;
    background-color: #ffffff;
    border-radius: .6rem;
    height: 3.4rem;
    width: 100%;
}
.award-info .tabs li > span{
    background-color: #fda198;
    box-shadow: 0 0 3px 0px rgba(162,8,0,0.7);
    width: 100%;
    height: 2.6rem;
    line-height: 2.6rem;
    position: absolute;
    top: .4rem;
    left: 0rem;
    z-index: 10;
}
.award-info .tabs li > span:before{
    background: transparent url('images/tab-left.png') no-repeat scroll left 0;
    left: -1rem;
}
.award-info .tabs li > span:after{
    background: transparent url('images/tab-right.png') no-repeat scroll left 0;
    right: -1rem;
}
.award-info .tabs li > span:before,
.award-info .tabs li > span:after{
    content: '';
    background-size: auto 3.2rem;
    height: 3.2rem;
    width: 1rem;
    position: absolute;
    top: -3px;
}
.award-info .tabs li .arrow{
    background: transparent url('images/tab-arrow.png') no-repeat scroll 0 0;
