@charset "UTF-8";

body { background: #ebebeb;}
.list {}
.list h1 { height: 0.92rem; line-height: 0.92rem; text-indent: 0.37rem; font-size: 0.34rem; background-color: #f9f9f9;}
.list-box {}
.list-list {}
.list-list > li { height: 2.13rem; margin-top: 0.22rem; background: url(../../images/header/bg01.jpg);}
.list-list> li > a { display: block; color: #333;}
.list-img { width: 2.92rem; height: 2.13rem; float: left;}
.list-img img { width: 100%; height: 100%;}
.list-msg { width: 4.22rem; float: right;}
.list-msg dt { height: 0.92rem; line-height: 0.92rem; overflow: hidden; font-size: 0.34rem;}
.list-msg dd { height: 1.2rem; overflow: hidden;}
.list-msg dd > span { display: inline-block; width: 1.78rem; height: 0.42rem; line-height: 0.42rem; border: solid 0.01rem #e96357; color: #e96357; font-size: 0.24rem; margin: 0 0.2rem 0.1rem 0; float: left; text-align: center; border-radius: 0.05rem;}

